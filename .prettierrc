{
	"arrowParens": "always",
	"bracketSpacing": false,
	"endOfLine": "lf",
	"useTabs": true,
	"trailingComma": "all",
	"tabWidth": 2,
	"singleQuote": false,
	"semi": true,
	"printWidth": 80,
	"htmlWhitespaceSensitivity": "css",
	"jsxBracketSameLine": false,
	"jsxSingleQuote": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
	"importOrderSortSpecifiers": true
}
